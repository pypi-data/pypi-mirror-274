<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:om="http://www.omel.es/Schemas" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:complexType name="MensajeDesagregacionesMCResponse">
		<xsd:sequence>
			<xsd:element ref="om:MensajeDesagregacionesMC"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="MensajeDesagregacionesMC" type="om:tMensajeDesagregacionesMC">
		<xsd:annotation>
			<xsd:documentation>Conjunto de desagregaciones del Mercado Continuo</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DesagregacionMC">
		<xsd:annotation>
			<xsd:documentation>Describe los datos de cada una de las desagregaciones individuales. Habrá uno por unidad portfolio</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdDesagregacion" type="om:tId">
					<xsd:annotation>
						<xsd:documentation>Identificativo de la desagregación dentro del fichero, definido por quien envía el fichero (único por mensaje).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Periodo">
					<xsd:annotation>
						<xsd:documentation>Periodo a desagregar</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="fecha" type="xsd:date" use="required">
							<xsd:annotation>
								<xsd:documentation>Fecha del periodo</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="num" use="required">
							<xsd:annotation>
								<xsd:documentation>Número del periodo</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:nonNegativeInteger">
									<xsd:maxInclusive value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad portfolio</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="UniEnergia" type="om:tUniEnergia" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Código "MWh" por defecto</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Comentario" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Comentario libre</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="v" use="required">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="om:Detalle" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Detalle de desagregaciones. Habrá uno por unidad ofertante ordinaria</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Detalle">
		<xsd:annotation>
			<xsd:documentation>Detalle de desagregaciones. Habrá uno por unidad ofertante ordinaria</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad ofertante ordinaria</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Ctn" type="om:tCantidad">
					<xsd:annotation>
						<xsd:documentation>Cantidad de Energía, con signo positivo</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Blq">
		<xsd:annotation>
			<xsd:documentation>Bloques horarios de la desagregación</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Per" type="om:tPeriodo">
					<xsd:annotation>
						<xsd:documentation>Periodo horario del día</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="tMensajeDesagregacionesMC">
		<xsd:annotation>
			<xsd:documentation>Tipo complejo para las desagregaciones del Mercado Continuo</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envía el mensaje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="om:DesagregacionMC" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Describe los datos de cada una de las desagregaciones individuales. Habrá uno por unidad portfolio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de  las desagregaciones de Intradiario Continuo</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
