<xsd:schema xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajePreciosGasNatural" type="om:tMensajePreciosGasNatural">
		<xsd:annotation>
			<xsd:documentation>Mensaje de precios del gas natural</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="tMensajePreciosGasNatural">
		<xsd:annotation>
			<xsd:documentation>Tipo global para precios del gas natural</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del emisor que envía el mensaje (OM)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sesion">
				<xsd:annotation>
					<xsd:documentation>Sesión</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="fecha" type="xsd:date" use="required">
						<xsd:annotation>
							<xsd:documentation>Fecha de la sesión</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="num" use="optional">
						<xsd:annotation>
							<xsd:documentation>Número de sesión</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:nonNegativeInteger">
								<xsd:maxInclusive value="25"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="UniPrecio" type="om:tUniPrecio" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Código "cEURO/kWh"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prc" type="om:tPrecioNegativo">
				<xsd:annotation>
					<xsd:documentation>Precio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrcRef" type="om:tPrecioNegativo">
				<xsd:annotation>
					<xsd:documentation>Precio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CUA" type="om:tPrecioNegativo">
				<xsd:annotation>
					<xsd:documentation>Precio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Ajuste" type="om:tSiNo">
				<xsd:annotation>
					<xsd:documentation>Precio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de precios del gas natural</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
