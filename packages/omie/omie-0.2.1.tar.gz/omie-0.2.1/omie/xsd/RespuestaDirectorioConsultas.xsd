<xsd:schema targetNamespace="http://www.omel.es/Schemas" xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:complexType name="RespuestaDirectorioConsultasResponse">
		<xsd:sequence>
			<xsd:element name="RespuestaDirectorioConsultas" type="om:tRespuestaDirectorioConsultas"/>
		</xsd:sequence>
	</xsd:complexType>		
	<xsd:complexType name="tRespuestaDirectorioConsultas">
		<xsd:annotation>
			<xsd:documentation>Tipo complejo para la respuesta del directorio de las Consultas Disponibles</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Seccion" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Sección o apartado al que pertenecen las consultas</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="om:Consultas" maxOccurs="unbounded"/>
					</xsd:sequence>
					<xsd:attribute name="v" use="required">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="255"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="RespuestaDirectorioConsultas" type="om:tRespuestaDirectorioConsultas">
		<xsd:annotation>
			<xsd:documentation>Conjunto de respuestas a las Consultas Disponibles</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Consultas">
		<xsd:annotation>
			<xsd:documentation>Consultas dentro de una sección</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="CodConsulta" type="om:tCodConsulta">
					<xsd:annotation>
						<xsd:documentation>Código de Consulta</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Titulo">
					<xsd:annotation>
						<xsd:documentation>Título de Consulta</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="v" use="required">
							<xsd:annotation>
								<xsd:documentation>Valor</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TipoConsulta" type="om:tTipoConsulta">
					<xsd:annotation>
						<xsd:documentation>Tipo de Consulta</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:annotation>
		<xsd:documentation>Esquema de la respuesta del directorio de las Consultas Disponibles</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
