<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:om="http://www.omel.es/Schemas" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajeFicherosNuevosFact" type="om:tMensajeFicherosNuevosFact">
		<xsd:annotation>
			<xsd:documentation>Ficheros nuevos de Facturación a descargar</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="tMensajeFicherosNuevosFact">
		<xsd:annotation>
			<xsd:documentation>Tipo complejo para los ficheros nuevos de Facturación a descargar</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UltimoSecuencial" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Valor del último número secuencial al cual se ha actualizado el cliente.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:nonNegativeInteger">
								<xsd:totalDigits value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="UltimaFecha" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Valor de la última fecha/hora de vigencia a la cual se ha actualizado el cliente.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DescargaPDF" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Valor que al seleccionar "SI", indica en la respuesta, si dicho fichero contiene PDF o F64</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v" type="om:tAtrSiNo">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="AmbitoDocumento" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Lista de ámbitos de documentos (FACTDIA, FACTMES, ...) para los cuales se solicitan ficheros nuevos</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v" use="required">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="30"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de ficheros nuevos de Facturación a descargar</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
