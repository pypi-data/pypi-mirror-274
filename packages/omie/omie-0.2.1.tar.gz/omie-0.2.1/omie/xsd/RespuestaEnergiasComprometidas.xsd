<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:om="http://www.omel.es/Schemas" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="RespuestaEnergiasComprometidas" type="om:tRespuestaEnergiasComprometidas">
		<xsd:annotation>
			<xsd:documentation>Respuestas de Energías comprometidas</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Respuesta">
		<xsd:annotation>
			<xsd:documentation>Código y texto de respuesta</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="CodigoRespuesta">
					<xsd:annotation>
						<xsd:documentation>Código de respuesta</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="v" type="xsd:string" use="required">
							<xsd:annotation>
								<xsd:documentation>Valor</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TextoRespuesta" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Textos descriptivos de la respuesta</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RespuestaSerie">
		<xsd:annotation>
			<xsd:documentation>Respuestas energías comprometidas</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdEnergiasComprometidas" type="om:tId">
					<xsd:annotation>
						<xsd:documentation>Identificativo dentro del fichero, definido por quien envía el fichero (único por mensaje). </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="om:Respuesta" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="tRespuestaEnergiasComprometidas">
		<xsd:annotation>
			<xsd:documentation>Tipo complejo para las respuestas de Energías Comprometidas</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación de la respuesta (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envía la respuesta (OMEL)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdMensajeOrigen" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje que se envió (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensajeOrigen" type="om:tVersionMensaje" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje que se envió (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitenteOrigen" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envió el mensaje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="om:Respuesta" maxOccurs="unbounded"/>
			<xsd:element name="ResumenEnergiasComprometidas">
				<xsd:annotation>
					<xsd:documentation>Datos resumen de proceso de las energías comprometidas</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="NumUnidadesProcesadas">
							<xsd:annotation>
								<xsd:documentation>Número total de energías comprometidas procesadas</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="v" type="xsd:nonNegativeInteger" use="required">
									<xsd:annotation>
										<xsd:documentation>Valor</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="NumUnidadesCorrectas">
							<xsd:annotation>
								<xsd:documentation>Número total de energías comprometidas correctas</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="v" type="xsd:nonNegativeInteger" use="required">
									<xsd:annotation>
										<xsd:documentation>Valor</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="om:RespuestaSerie" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Respuestas energías comprometidas</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de respuestas de Energ젳 comprometidas</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
