<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.4.4">
	<xsd:annotation>
		<xsd:documentation>Tipos básicos de OMEL</xsd:documentation>
	</xsd:annotation>
	<!-- O M E L   -   T I P O S   B A S I C O S -->
	<xsd:complexType name="tId">
		<xsd:annotation>
			<xsd:documentation>Código que identifica un elemento de forma única dentro del documento.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string"/>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIdNum">
		<xsd:annotation>
			<xsd:documentation>Número que identifica un elemento de forma única dentro del documento.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger"/>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tVersionMensaje">
		<xsd:annotation>
			<xsd:documentation>Número de versión de un mensaje</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="xsd:nonNegativeInteger" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tIdRemitente">
		<xsd:annotation>
			<xsd:documentation>Tipo que identifica un remitente, como código de agente, código EIC, etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="16"/>
					<xsd:pattern value="(\p{Lu}|\p{N}|\p{Pd}|\p{Pc}|\p{Zs}){1,16}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tFecha">
		<xsd:annotation>
			<xsd:documentation>Fecha </xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="xsd:date" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tFechaHora">
		<xsd:annotation>
			<xsd:documentation>Fecha y hora</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="xsd:dateTime" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tTerminoFijo">
		<xsd:annotation>
			<xsd:documentation>Tipo en el que se expresa el término fijo</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger">
					<xsd:totalDigits value="9"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tTerminoVariable">
		<xsd:annotation>
			<xsd:documentation>Tipo en el que se expresa el término variable</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:minInclusive value="0"/>
					<xsd:fractionDigits value="3"/>
					<xsd:maxInclusive value="999999999.999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tGradiente">
		<xsd:annotation>
			<xsd:documentation>Tipo en el que se expresan los gradientes</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="99999.9"/>
					<xsd:fractionDigits value="1"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tUnidad">
		<xsd:annotation>
			<xsd:documentation>Identificación de unidad ofertante o física</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de la unidad</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="14"/>
					<xsd:pattern value="(\p{Lu}|\p{N}|\p{Pd}|\p{Pc}|\p{Zs}|/){1,14}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="codMin" use="optional">
			<xsd:annotation>
				<xsd:documentation>Código del ministerio</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="50"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="area" type="om:tAtrArea" use="optional">
			<xsd:annotation>
				<xsd:documentation>Área de la unidad</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tipo" type="om:tAtrTipoUnidad" use="optional">
			<xsd:annotation>
				<xsd:documentation>Tipo de unidad</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tPeriodo">
		<xsd:annotation>
			<xsd:documentation>Número de periodo dentro de un intervalo temporal (hora)</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:positiveInteger">
					<xsd:maxInclusive value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCantidad">
		<xsd:annotation>
			<xsd:documentation>Cantidad de energía</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:fractionDigits value="3"/>
					<xsd:minInclusive value="-99999.999"/>
					<xsd:maxInclusive value="99999.999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCantidadSinMax">
		<xsd:annotation>
			<xsd:documentation>Cantidad de energía (sin maximo)</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:fractionDigits value="3"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tPrecio">
		<xsd:annotation>
			<xsd:documentation>Precio.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:fractionDigits value="3"/>
					<xsd:minInclusive value="0"/>
					<xsd:totalDigits value="8"/>
					<xsd:maxInclusive value="99999.999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tPrecioNegativo">
		<xsd:annotation>
			<xsd:documentation>Precio negativo</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:fractionDigits value="3"/>
					<xsd:minInclusive value="-99999.999"/>
					<xsd:totalDigits value="8"/>
					<xsd:maxInclusive value="99999.999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIntervaloTiempo">
		<xsd:annotation>
			<xsd:documentation>Intervalo de tiempo expresado por una fecha y hora inicial y otra final, en formato ISO 8601.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[2-9][0-9][0-9][0-9][\-][0-1][0-9][\-][0-3][0-9][T][0-2][0-9][:][0-6][0-9](Z|[\+][0-2][0-9][:][0-6][0-9])[/][2-9][0-9][0-9][0-9][\-][0-1][0-9][\-][0-3][0-9][T][0-2][0-9][:][0-6][0-9](Z|[\+][0-2][0-9][:][0-6][0-9])"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCodTransaccion">
		<xsd:annotation>
			<xsd:documentation>Código y versión de una transacción (oferta, ejecución, declaración)</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="codigo" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de la transacción</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="9999999999"/>
					<xsd:totalDigits value="10"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="version" use="optional">
			<xsd:annotation>
				<xsd:documentation>Versión de la transacción</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger">
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="999"/>
					<xsd:totalDigits value="3"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIdPrograma">
		<xsd:annotation>
			<xsd:documentation>Identificativo de programa</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrIdPrograma" use="required">
			<xsd:annotation>
				<xsd:documentation>Identificativo del programa</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="t" type="om:tAtrTipoPrograma" use="required">
			<xsd:annotation>
				<xsd:documentation>Tipo de programa</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fuente" type="om:tAtrFuente" use="required">
			<xsd:annotation>
				<xsd:documentation>Fuente que originó el programa</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCodConsulta">
		<xsd:annotation>
			<xsd:documentation>Código de consulta</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger">
					<xsd:maxInclusive value="9999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIdioma">
		<xsd:annotation>
			<xsd:documentation>Código de idioma</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tSegmento">
		<xsd:annotation>
			<xsd:documentation>Tipo de segmento en anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="8"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tSesionAnotaciones">
		<xsd:annotation>
			<xsd:documentation>Número de sesión de intradiario para anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:nonNegativeInteger">
					<xsd:totalDigits value="2"/>
					<xsd:minInclusive value="0"/>
					<xsd:maxInclusive value="25"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tSignoImp">
		<xsd:annotation>
			<xsd:documentation>Signo de Importes en anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrSignoImp" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tSignoEne">
		<xsd:annotation>
			<xsd:documentation>Signo de Energías en anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrSignoEne" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tSignoPrecio">
		<xsd:annotation>
			<xsd:documentation>Signo de Precios en anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrSignoPrecio" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tCodigo">
		<xsd:annotation>
			<xsd:documentation>Códigos varios</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="8"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCuenta">
		<xsd:annotation>
			<xsd:documentation>Código de cuenta de compensacion en anotaciones</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="8"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tRespuesta">
		<xsd:annotation>
			<xsd:documentation>Código y texto de respuesta</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CodigoRespuesta">
				<xsd:annotation>
					<xsd:documentation>Código de respuesta</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v" type="xsd:string" use="required">
						<xsd:annotation>
							<xsd:documentation>Valor</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TextoRespuesta" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Textos descriptivos de la respuesta</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tAgente">
		<xsd:annotation>
			<xsd:documentation>Identificación del Agente</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de Agente</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="5"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="desc" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Descripción del agente</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="CIF" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>CIF del agente</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tNudo">
		<xsd:annotation>
			<xsd:documentation>Identificación de Nudo físico de la red</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de nudo</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="7"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="tipo" type="om:tAtrTipoNudo" use="required">
			<xsd:annotation>
				<xsd:documentation>Tipo de nudo</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIdGrupoEmpresarial">
		<xsd:annotation>
			<xsd:documentation>Identificación de Grupo Empresarial</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de grupo empresarial</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="7"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tIdEmpresa">
		<xsd:annotation>
			<xsd:documentation>Identificación de Empresa</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de empresa</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="8"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tImporte">
		<xsd:annotation>
			<xsd:documentation>Tipo en el que se expresan los importes</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:totalDigits value="16"/>
					<xsd:fractionDigits value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCodSubasta">
		<xsd:annotation>
			<xsd:documentation>Código de Subasta por la Interconexión</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de subasta</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tCodProducto">
		<xsd:annotation>
			<xsd:documentation>Código de Producto</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de producto</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tTipoProducto">
		<xsd:annotation>
			<xsd:documentation>Tipo de Producto</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" use="required">
			<xsd:annotation>
				<xsd:documentation>Código de tipo de producto</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<!--O M E L   -   L I S T A S   D E   V A L O R E S-->
	<xsd:complexType name="tMercado">
		<xsd:annotation>
			<xsd:documentation>Tipo de mercado</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrMercado" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tUniEnergia">
		<xsd:annotation>
			<xsd:documentation>Unidades de energía</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrUniEnergia" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tUniMonetaria">
		<xsd:annotation>
			<xsd:documentation>Unidades monetarias</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrUniMonetaria" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tUniPrecio">
		<xsd:annotation>
			<xsd:documentation>Unidades de precio</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrUniPrecio" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tUniGradiente">
		<xsd:annotation>
			<xsd:documentation>Unidades para gradientes</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrUniGradiente" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tClaseOferta">
		<xsd:annotation>
			<xsd:documentation>Clases de Ofertas</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrClaseOferta" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tTipoTransaccion">
		<xsd:annotation>
			<xsd:documentation>Tipo y Subtipo de transacción</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="tipo" type="om:tAtrTipoTransaccion" use="required">
			<xsd:annotation>
				<xsd:documentation>Tipo de transacción</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="subTipo" type="om:tAtrSubtipoTransaccion" use="required">
			<xsd:annotation>
				<xsd:documentation>Subtipo de transacción</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="tAgregacion">
		<xsd:annotation>
			<xsd:documentation>Tipo de agregacion</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrAgregacion" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tSiNo">
		<xsd:annotation>
			<xsd:documentation>Tipo de elemento "booleano" (S, Y, N)</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrSiNo" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tTipoConsulta">
		<xsd:annotation>
			<xsd:documentation>Tipo de Consulta</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrTipoConsulta" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tTipoDescarga">
		<xsd:annotation>
			<xsd:documentation>Tipo de Descarga</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="v" type="om:tAtrTipoDescarga" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="tTipoCasacion">
