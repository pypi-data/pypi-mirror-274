		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Casación normal</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAtrTipoContrato">
		<xsd:annotation>
			<xsd:documentation>Atributo para tipo de Contrato Bilateral</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>CB Internacional entre agentes en una frontera</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Tránsito internacional entre agentes</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>CB internacional con un agente instrumental</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation>Tránsito internacional con un agente instrumental</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="5">
				<xsd:annotation>
					<xsd:documentation>CB nacional entre comercializadoras</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="6">
				<xsd:annotation>
					<xsd:documentation>CB nacional entre productor y consumidor</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="7">
				<xsd:annotation>
					<xsd:documentation>CB nacional entre productor en RE y consumidor</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="8">
				<xsd:annotation>
					<xsd:documentation>Asimilación de ofertas a bilaterales</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAtrTipoResponsableCostes">
		<xsd:annotation>
			<xsd:documentation>Atributo para tipo de Agente responsable de los costes de un Contrato Bilateral</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="V">
				<xsd:annotation>
					<xsd:documentation>Agente de Venta</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="C">
				<xsd:annotation>
					<xsd:documentation>Agente de Compra</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A">
				<xsd:annotation>
					<xsd:documentation>Ambos</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAtrTipoNudo">
		<xsd:annotation>
			<xsd:documentation>Atributo para tipo de nudo</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>Consumo</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Suministro</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAtrClaseOfertaAnotacion">
		<xsd:annotation>
			<xsd:documentation>Atributo para clase de oferta de una anotación (C, V, Z)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="C">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>C: Compra</Title>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="V">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>V: Venta</Title>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z">
				<xsd:annotation>
					<xsd:documentation>Z: Instrumental</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAtrEstadoLiq">
		<xsd:annotation>
			<xsd:documentation>Atributo para estado de una liquidación (P, D)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>P: Provisional</Title>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="D">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>D: Definitiva</Title>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--ELEMENTOS SIMPLES-->
	<xsd:element name="SeleccionIdioma" type="om:tIdioma">
		<xsd:annotation>
			<xsd:documentation>Elemento para la selección del idioma</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CodConsulta" type="om:tCodConsulta">
		<xsd:annotation>
			<xsd:documentation>Elemento para la configuración de una consulta</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TipoDescarga" type="om:tTipoDescarga">
		<xsd:annotation>
			<xsd:documentation>Elemento para la selección del tipo de descarga de ficheros</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="FechaHoraResponse">
		<xsd:sequence>
			<xsd:element name="FechaHora" type="om:tFechaHora"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
