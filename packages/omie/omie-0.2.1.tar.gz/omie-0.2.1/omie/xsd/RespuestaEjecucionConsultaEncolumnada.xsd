<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSPY v5 rel. 3 U (http://www.xmlspy.com) by F. García Julián (OMEL S.A.) -->
<xsd:schema xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.3">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:complexType name="RespuestaEjecucionConsultaEncolumnadaResponse">
		<xsd:sequence>
			<xsd:element name="RespuestaEjecucionConsultaEncolumnada" type="om:tRespuestaEjecucionConsultaEncolumnada"/>
		</xsd:sequence>
	</xsd:complexType>		
	<xsd:element name="RespuestaEjecucionConsultaEncolumnada" type="om:tRespuestaEjecucionConsultaEncolumnada">
		<xsd:annotation>
			<xsd:documentation>Respuesta a la ejecución de una consulta encolumnada</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="tRespuestaEjecucionConsultaEncolumnada">
		<xsd:annotation>
			<xsd:documentation>Tipo básico para respuesta a la ejecución de una consulta encolumnada</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CodConsulta" type="om:tCodConsulta">
				<xsd:annotation>
					<xsd:documentation>Código de Consulta</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Parametros">
				<xsd:annotation>
					<xsd:documentation>Parámetros de llamada a la consulta</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence minOccurs="0" maxOccurs="unbounded">
						<xsd:choice>
							<xsd:element name="Txt">
								<xsd:annotation>
									<xsd:documentation>Parámetro de tipo texto</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" use="required">
										<xsd:annotation>
											<xsd:documentation>Valor
</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string"/>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Num">
								<xsd:annotation>
									<xsd:documentation>Parámetro de tipo númerico</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:decimal" use="required">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Fec">
								<xsd:annotation>
									<xsd:documentation>Parámetro de tipo fecha</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:date" use="required">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Ses">
								<xsd:annotation>
									<xsd:documentation>Parámetro de tipo Sesión de Intradiario</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" use="required">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:nonNegativeInteger">
												<xsd:minInclusive value="1"/>
												<xsd:maxInclusive value="25"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Agregados" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence maxOccurs="unbounded">
						<xsd:choice>
							<xsd:element name="Txt">
								<xsd:annotation>
									<xsd:documentation>Agregado de tipo texto</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string"/>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Num">
								<xsd:annotation>
									<xsd:documentation>Agregado de tipo numérico</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:decimal" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Fec">
								<xsd:annotation>
									<xsd:documentation>Agregado de tipo fecha</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:date" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Feh">
								<xsd:annotation>
									<xsd:documentation>Agtregado de tipo Fecha / Hora</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:dateTime" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Hor">
								<xsd:annotation>
									<xsd:documentation>Agregado de tipo hora</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:time" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Fila" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Filas resultado de la consulta</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence maxOccurs="unbounded">
						<xsd:choice>
							<xsd:element name="Txt">
								<xsd:annotation>
									<xsd:documentation>Columna de tipo texto</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string"/>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Num">
								<xsd:annotation>
									<xsd:documentation>Columna de tipo numérico</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:decimal" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Fec">
								<xsd:annotation>
									<xsd:documentation>Columna de tipo fecha</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:date" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Feh">
								<xsd:annotation>
									<xsd:documentation>Columna de tipo Fecha / Hora</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:dateTime" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Hor">
								<xsd:annotation>
									<xsd:documentation>Columna de tipo hora</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="n" use="required">
										<xsd:annotation>
											<xsd:documentation>Nombre</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="v" type="xsd:time" use="optional">
										<xsd:annotation>
											<xsd:documentation>Valor</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Respuesta" type="om:tRespuesta" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Código y texto de respuesta</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de respuesta a la ejecución de una consulta encolumnada</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
