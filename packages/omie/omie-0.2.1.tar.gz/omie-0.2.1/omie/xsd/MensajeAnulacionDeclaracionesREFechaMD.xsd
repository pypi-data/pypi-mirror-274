<xsd:schema targetNamespace="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:om="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajeAnulacionDeclaracionesREFechaMD" type="om:tMensajeAnulacionDeclaracionesREFechaMD">
		<xsd:annotation>
			<xsd:documentation>Anulación de Declaraciones de Régimen Especial al Mercado Diario para una Fecha</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ParametrosAnulacion">
		<xsd:annotation>
			<xsd:documentation>Describe los parámetros de anulación de Declaraciones de Régimen Especial</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdAnulacion" type="om:tId">
					<xsd:annotation>
						<xsd:documentation>Identificativo de la anulación dentro del fichero, definido por quien envía el fichero (único por mensaje).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad de Régimen especial</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Validez">
					<xsd:annotation>
						<xsd:documentation>Validez</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="TipoMercado" type="om:tMercado">
								<xsd:annotation>
									<xsd:documentation>MD: Mercado Diario</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Sesion">
								<xsd:annotation>
									<xsd:documentation>Sesión</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="fecha" type="xsd:date" use="required">
										<xsd:annotation>
											<xsd:documentation>Fecha de la sesión
</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="tMensajeAnulacionDeclaracionesREFechaMD">
		<xsd:annotation>
			<xsd:documentation>Tipo global para Anulación de Declaraciones de Régimen Especial al Mercado Diario para una Fecha</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envía la solicitud de Anulación.  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="om:ParametrosAnulacion" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de anulación de Declaraciones de Régimen Especial al Mercado Diario para una Fecha</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
