<xsd:schema xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajeEnergiasComprometidas" type="om:tMensajeEnergiasComprometidas">
		<xsd:annotation>
			<xsd:documentation>Mensaje de Energías Comprometidas</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnergiasComprometidas">
		<xsd:annotation>
			<xsd:documentation>Describe los datos de todas las energias comprometidas de las unidades de un Agente para una fecha dada</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdEnergiasComprometidas" type="om:tId">
					<xsd:annotation>
						<xsd:documentation>Identificativo del conjunto de energías comprometidas dentro del fichero, definido por quien envía el fichero (único por mensaje).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Validez">
					<xsd:annotation>
						<xsd:documentation>Validez</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="TipoMercado" type="om:tMercado">
								<xsd:annotation>
									<xsd:documentation>MD: Mercado Diario</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Sesion">
								<xsd:annotation>
									<xsd:documentation>Incluye Fecha </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="fecha" type="xsd:date" use="required"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="UniEnergia" type="om:tUniEnergia" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Código "MWh" por defecto</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="om:Detalle" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Detalle de las energías comprometidas. Habrá uno por Unidad Ofertante y número de comunidación.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="tMensajeEnergiasComprometidas">
		<xsd:annotation>
			<xsd:documentation>Tipo global para Energías Comprometidas</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envía la oferta.  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="om:EnergiasComprometidas" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Detalle">
		<xsd:annotation>
			<xsd:documentation>Detalle de las Energías Comprometidas para una Fecha, Unidad Ofertante u Número de Comunicación</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Dia" type="om:tFecha">
					<xsd:annotation>
						<xsd:documentation>Fecha</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad ofertante.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="NumComunicacion" type="om:tIdNum">
					<xsd:annotation>
						<xsd:documentation>Número de Comunicación</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="om:Blq" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Bloques Horarios de Energías comprometidas</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Blq">
		<xsd:annotation>
			<xsd:documentation>Bloques Horarios de Energía Comprometida</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Per" type="om:tPeriodo">
					<xsd:annotation>
						<xsd:documentation>Periodo horario del día</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Ctn" type="om:tCantidad">
					<xsd:annotation>
						<xsd:documentation>Energía comprometida, con su signo.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:annotation>
		<xsd:documentation>Esquema correspondiente al mensaje de ofertas del Mercado Diario</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
