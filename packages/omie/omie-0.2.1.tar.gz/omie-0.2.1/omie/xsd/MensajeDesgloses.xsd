<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSPY v5 rel. 2 U (http://www.xmlspy.com) by F. Poveda (OMEL S.A.) -->
<xsd:schema targetNamespace="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema"  xmlns:om="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajeDesgloses" type="om:tMensajeDesgloses">
		<xsd:annotation>
			<xsd:documentation>Conjunto de desgloses por unidad de producción</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Desglose">
		<xsd:annotation>
			<xsd:documentation>Describe los datos de cada uno de los desgloses individuales. Habrá uno por unidad de producción</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdDesglose" type="om:tId">
					<xsd:annotation>
						<xsd:documentation>Identificativo del desglose dentro del fichero, definido por quien envía el fichero (único por mensaje).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IdCBF" type="om:tIdNum" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Código del Contrato Bilateral Físico, sólo para desgloses de bilaterales en el Mercado Diario.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad de producción</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Validez">
					<xsd:annotation>
						<xsd:documentation>Validez</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="TipoMercado" type="om:tMercado">
								<xsd:annotation>
									<xsd:documentation>MD: Mercado Diario
MI: Mercado Intradiario</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Sesion">
								<xsd:annotation>
									<xsd:documentation>Sesión para la que se desglosa</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="fecha" type="xsd:date" use="required">
										<xsd:annotation>
											<xsd:documentation>Fecha de la sesión</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="num" use="optional">
										<xsd:annotation>
											<xsd:documentation>Número de la sesión</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:nonNegativeInteger">
												<xsd:maxInclusive value="25"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="UniEnergia" type="om:tUniEnergia" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Código "MWh" por defecto</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Comentario" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Comentario libre</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="v" use="required">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="om:Detalle" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Detalle">
		<xsd:annotation>
			<xsd:documentation>Detalle de desgloses. Habrá uno por unidad física</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Dia" type="om:tFecha">
					<xsd:annotation>
						<xsd:documentation>Fecha</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IdUnidad" type="om:tUnidad">
					<xsd:annotation>
						<xsd:documentation>Código de unidad de física </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="om:Blq" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Blq">
		<xsd:annotation>
			<xsd:documentation>Bloques </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Per" type="om:tPeriodo">
					<xsd:annotation>
						<xsd:documentation>Periodo horario del día</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Ctn" type="om:tCantidad">
					<xsd:annotation>
						<xsd:documentation>Cantidad de Energía</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="tMensajeDesgloses">
		<xsd:annotation>
			<xsd:documentation>Tipo complejo para los desgloses del PBF por unidad de producción</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje (libre para el creador del mensaje)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del Agente que envía el mensaje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="om:Desglose" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de  los desgloses del PBF por unidad de producción</xsd:documentation>
	</xsd:annotation>
</xsd:schema>
