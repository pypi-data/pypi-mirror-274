<xsd:schema xmlns:om="http://www.omel.es/Schemas" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.omel.es/Schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:include schemaLocation="tiposOMEL.xsd"/>
	<xsd:element name="MensajePrecios" type="om:tMensajePrecios">
		<xsd:annotation>
			<xsd:documentation>Mensaje de precios de casación</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="tMensajePrecios">
		<xsd:annotation>
			<xsd:documentation>Tipo global para precios de casación </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdMensaje" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificación del mensaje </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VerMensaje" type="om:tVersionMensaje">
				<xsd:annotation>
					<xsd:documentation>Versión del mensaje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FechaMensaje" type="om:tFechaHora">
				<xsd:annotation>
					<xsd:documentation>Fecha y hora de generación de los datos para el mensaje (no de envío del mensaje en sí)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdRemitente" type="om:tIdRemitente">
				<xsd:annotation>
					<xsd:documentation>Código del emisor que envía el mensaje (OM)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdPrecio">
				<xsd:annotation>
					<xsd:documentation>Marginal PDBC, Marginal PIBC</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="v" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:NMTOKEN">
								<xsd:enumeration value="PDBC">
									<xsd:annotation>
										<xsd:documentation>PDBC: Marginal PDBC</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="PIBC">
									<xsd:annotation>
										<xsd:documentation>PIBC: Marginal PIBC</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TipoMercado" type="om:tMercado">
				<xsd:annotation>
					<xsd:documentation>MD: Mercado Diario
MI: Mercado Intradiario</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sesion">
				<xsd:annotation>
					<xsd:documentation>Sesión</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="fecha" type="xsd:date" use="required">
						<xsd:annotation>
							<xsd:documentation>Fecha de la sesión</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="num" use="optional">
						<xsd:annotation>
							<xsd:documentation>Número de sesión</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:nonNegativeInteger">
								<xsd:maxInclusive value="25"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SerieTemporal" type="om:tSerieTemporalPrecios" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Series temporales para los precios de cada area</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tSerieTemporalPrecios">
		<xsd:annotation>
			<xsd:documentation>Serie temporal</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdSerieTemporal" type="om:tId">
				<xsd:annotation>
					<xsd:documentation>Identificativo de serie dentro del fichero.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdArea" type="om:tId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identificativo de Área a la que aplica el precio. En caso de no existir este elemento, se entiende que corresponde a la zona de España.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UniPrecio" type="om:tUniPrecio" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Código "cEURO/kWh"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Periodo" type="om:tPeriodoPrecios" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tPeriodoPrecios">
		<xsd:annotation>
			<xsd:documentation>Periodo de la casación</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Dia" type="om:tFecha">
				<xsd:annotation>
					<xsd:documentation>Fecha del Periodo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Val" type="om:tValPrecios" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tValPrecios">
		<xsd:annotation>
			<xsd:documentation>Valor individual</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Per" type="om:tPeriodo">
				<xsd:annotation>
					<xsd:documentation>Periodo horario del día</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prc" type="om:tPrecioNegativo">
				<xsd:annotation>
					<xsd:documentation>Precio</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>Esquema de precios de casación </xsd:documentation>
	</xsd:annotation>
</xsd:schema>
