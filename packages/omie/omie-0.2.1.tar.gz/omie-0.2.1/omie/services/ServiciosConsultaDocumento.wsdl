<?xml version="1.0" encoding="UTF-8"?>


<wsdl:definitions xmlns:apachesoap="http://xml.apache.org/xml-soap" 
	xmlns:impl="https://www.mercado.omie.es/jsiom/webServices/ServiciosConsulta"
