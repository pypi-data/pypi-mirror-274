{
  "name": "mimicri",
  "version": "0.1.0",
  "description": "Masking and mixing for interactive counterfactual recombined images",
  "author": "",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//mimicri.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../mimicri/labextension/ && rimraf ../mimicri/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "css-loader": "^7.1.1",
    "rimraf": "^2.6.1",
    "webpack": "^5",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4 || ^6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@types/three": "^0.152.1",
    "allotment": "^1.19.0",
    "classnames": "^2.3.2",
    "crossfilter2": "^1.5.4",
    "d3": "^7.8.4",
    "n": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "style-loader": "^4.0.0",
    "three": "^0.155.0",
    "yarn": "^1.22.19"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../mimicri/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    },
    "_build": {
      "load": "static/remoteEntry.6243434dfdfae17b833d.js",
      "extension": "./extension"
    }
  }
}
